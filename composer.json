{
  "name": "saeedvir/laravel-advanced-attributes",
  "description": "Make Advanced Attributes For Any Laravel Models",
  "keywords": [
    "saeedvir",
    "laravel-advanced-attributes",
    "laravel advanced-attributes",
    "advanced attributes",
    "laravel",
    "laravel attributes package",
    "laravel advanced attributes package"
  ],
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "saeed abdollahian",
      "email": "saeed.es91@gmail.com",
      "homepage": "https://github.com/saeedvir",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "laravel/framework": "^9.0|^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "pestphp/pest-plugin-laravel": "^1.4.0|^2.0.0",
    "orchestra/testbench": "^7.0|^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Saeedvir\\LaravelAdvancedAttributes\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Saeedvir\\LaravelAdvancedAttributes\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Saeedvir\\LaravelAdvancedAttributes\\LaravelAdvancedAttributesServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/pest",
    "test-coverage": "vendor/bin/pest --coverage-html ./coverage"
  }
}
